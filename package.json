{
    "name": "vue-primevue-feedback-project",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "format": "prettier --write .",
        "prepare": "husky",
        "preinstall": "npx only-allow pnpm",
        "publish": "NODE_ENV=production pnpm run build && gh-pages -d dist --dotfiles",
        "review": "pnpx electron electron/main.cjs",
        "review:dev": "concurrently \"pnpm run dev\" \"wait-on http://localhost:5173 && pnpx electron electron/main.cjs\""
    },
    "dependencies": {
        "@vee-validate/rules": "^4.15.1",
        "chart.js": "^3.9.1",
        "pinia": "^2.1.6",
        "primeflex": "^3.3.0",
        "primeicons": "^6.0.0",
        "primevue": "^3.0.0",
        "uuid": "^9.0.0",
        "vee-validate": "^4.15.1",
        "vue": "^3.4.0",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "5.2.4",
        "concurrently": "^9.2.1",
        "electron": "^38.1.0",
        "electron-builder": "^26.0.12",
        "eslint": "8.57.1",
        "eslint-plugin-vue": "9.33.0",
        "gh-pages": "6.3.0",
        "husky": "9.1.7",
        "lint-staged": "16.1.6",
        "prettier": "3.6.2",
        "vite": "5.4.20",
        "wait-on": "^8.0.4"
    },
    "lint-staged": {
        "*.{js,vue}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css,html}": [
            "prettier --write"
        ]
    }
}